<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<AnchorPane fx:id="mapPane" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.aoopproject.MapController">
    <children>
      <ImageView fx:id="mapBG" fitHeight="700.0" fitWidth="900.0" pickOnBounds="true">
      </ImageView>
      <Pane fx:id="sceneView" layoutX="273.0" layoutY="8.0" prefHeight="686.0" prefWidth="619.0" styleClass="translucent-pane" stylesheets="@Admin.css">
         <children>
              <WebView fx:id="webView" prefHeight="686.0" prefWidth="619.0" visible="false" />
            <Button layoutX="507.0" layoutY="14.0" mnemonicParsing="false" onAction="#logOut" styleClass="glass-button" stylesheets="@button.css" text="Log Out">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Pane layoutX="7.0" layoutY="7.0" prefHeight="686.0" prefWidth="256.0" styleClass="translucent-pane" stylesheets="@Admin.css">
         <children>
            <Button fx:id="homePage" layoutX="5.0" layoutY="207.0" mnemonicParsing="false" onAction="#homePageSwitch" prefHeight="48.0" prefWidth="246.0" styleClass="glass-button" stylesheets="@button.css" text="Home page" />
            <Button fx:id="newsPortal" layoutX="5.0" layoutY="265.0" mnemonicParsing="false" onAction="#newsPortalSwitch" prefHeight="48.0" prefWidth="246.0" styleClass="glass-button" stylesheets="@button.css" text="News Portal" />
            <Button fx:id="map" layoutX="5.0" layoutY="319.0" mnemonicParsing="false" onAction="#mapSwitch" prefHeight="48.0" prefWidth="246.0" styleClass="glass-button" stylesheets="@button.css" text="Map" />
            <Button fx:id="eventHolidays" layoutX="5.0" layoutY="377.0" mnemonicParsing="false" onAction="#eventHolidaySwitch" prefHeight="48.0" prefWidth="246.0" styleClass="glass-button" stylesheets="@button.css" text="Event &amp; Holidays" />
         </children>
      </Pane>
    </children>
</AnchorPane>
